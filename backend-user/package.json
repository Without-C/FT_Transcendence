{
  "type": "module",
  "name": "backend-user",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch='(.ts$|.db$)' -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/env": "^5.0.2",
    "@fastify/jwt": "^9.0.4",
    "@fastify/sensible": "^6.0.0",
    "@prisma/client": "^6.3.1",
    "axios": "^1.7.9",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.3.0",
    "fastify-plugin": "^5.0.0",
    "ps": "^1.0.0",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/qs": "^6.9.18",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^2.0.0",
    "prisma": "^6.3.1",
    "ts-node": "^10.4.0",
    "typescript": "~5.6.3"
  }
}
